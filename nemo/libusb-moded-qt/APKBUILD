pkgname=libusb-moded-qt
pkgver=1.6
pkgrel=0
pkgdesc="A library of Qt5 bindings for usb_moded"
arch="all"
url="https://git.merproject.org/mer-core/libusb-moded-qt"
license="LGPL-2.1"
makedepends="qt5-qtbase-dev usb-moded-dev"
source="https://git.merproject.org/mer-core/$pkgname/-/archive/$pkgver/$pkgname-$pkgver.tar.bz2"

build() {
    cd "$builddir"
	qmake-qt5
	make
}

package() {
    cd "$builddir"
	make INSTALL_ROOT="$pkgdir/" install
}
